{
  "name": "vacinas-server",
  "version": "1.0.0",
  "description": "Backend application for Vacinas project",
  "main": "src/server.ts",
  "scripts": {
    "barrels:domain": "barrelsby --directory './src/domain' --delete --singleQuotes --location below",
    "barrels:mock": "barrelsby --directory './src/test/mock' --delete --singleQuotes",
    "barrels": "yarn barrels:domain && yarn barrels:mock",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register --exit --timeout 30000 src/test/index.ts",
    "lint": "eslint --fix --ext .js,.ts src",
    "seed": "ts-node -r tsconfig-paths/register src/test/seed/index.ts",
    "seed:test": "NODE_ENV=test ts-node -r tsconfig-paths/register src/test/seed/index.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "repository": "https://github.com/rodmagaldi/vacinas-server.git",
  "author": "Rodrigo Magaldi <rod.magaldi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cpf": "2.0.1",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "typedi": "0.8.0",
    "typeorm": "0.2.31",
    "typeorm-naming-strategies": "2.0.0",
    "typeorm-typedi-extensions": "0.2.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.15",
    "@types/express": "4.17.12",
    "@types/faker": "5.5.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.28",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "barrelsby": "2.2.0",
    "chai": "4.3.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "faker": "5.5.2",
    "mocha": "8.3.0",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": "12.x.x"
  }
}
